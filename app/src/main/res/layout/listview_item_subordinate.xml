<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/listview_item_subordinate_ll"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sw_40dp"
        android:padding="@dimen/sw_5dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/listview_item_subordinate_checkbox_iv"
            android:layout_width="@dimen/sw_20dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/sw_5dp"
            android:src="@drawable/login_checkbox_off" />

        <TextView
            android:id="@+id/listview_item_subordinate_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/sw_5dp"
            android:textColor="@color/black"
            android:textSize="@dimen/sw_15dp" />
    </LinearLayout>
</LinearLayout>
